{
  "name": "@jupyterlab/sharedb",
  "version": "0.1.0",
  "description": "Realtime collaboration with JupyterLab through ShareDB",
  "author": "Ian Rose",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*/*d.ts",
    "lib/*/*.js",
    "lib/*.d.ts",
    "lib/*.js"
  ],
  "directories": {
    "lib": "lib/"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "sharedb"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib",
    "watch": "tsc -w"
  },
  "dependencies": {
    "@jupyterlab/application": "^0.9.0",
    "@jupyterlab/apputils": "^0.9.0",
    "@jupyterlab/coreutils": "^0.9.0",
    "@jupyterlab/docmanager": "^0.9.0",
    "@jupyterlab/docregistry": "^0.9.0",
    "@jupyterlab/filebrowser": "^0.9.0",
    "@jupyterlab/services": "^0.48.0",
    "@phosphor/algorithm": "^1.1.1",
    "@phosphor/application": "^1.3.0",
    "@phosphor/coreutils": "^1.2.0",
    "@phosphor/disposable": "^1.1.1",
    "@phosphor/signaling": "^1.2.1",
    "@phosphor/widgets": "^1.3.0",
    "core-js": "^2.4.1",
    "es6-promise": "^4.1.0",
    "sharedb": "^1.0.0-beta.7"
  },
  "devDependencies": {
    "@types/node": "^7.0.11",
    "rimraf": "^2.5.2",
    "typescript": "~2.4.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ian-r-rose/jupyterlab-sharedb.git"
  },
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/ian-r-rose/jupyterlab-sharedb/issues"
  },
  "homepage": "https://github.com/ian-r-rose/jupyterlab-sharedb"
}
